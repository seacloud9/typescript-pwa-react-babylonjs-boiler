{
  "name": "sputnik-typescript-pwa-react-babylonjs",
  "version": "0.0.1",
  "description": "Sputnik -> a starter kit for React, Webpack (with Hot Module Replacement), PWA, HTTPS, React-BabylonJS, Typescript and Babel.",
  "keywords": [
    "react",
    "webpack",
    "pwa",
    "manifest",
    "react-babylonJS",
    "typescript",
    "babel",
    "emotion",
    "hmr",
    "starter",
    "boilerplate"
  ],
  "author": "Brendon Smith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seacloud9/typescript-pwa-react-babylonjs-boiler.git"
  },
  "bugs": {
    "url": "https://github.com/seacloud9/typescript-pwa-react-babylonjs-boiler/issues"
  },
  "homepage": "https://github.com/seacloud9/typescript-pwa-react-babylonjs-boiler",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p  --https  --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server  --https  --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.1.0",
    "express": "^4.17.1",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.8.0",
    "react": "^16.8.24",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.10",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "dependencies": {
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "babylonjs": "4.0.0-alpha.10",
    "babylonjs-gui": "4.0.0-alpha.10",
    "babylonjs-inspector": "4.0.0-alpha.10",
    "babylonjs-loaders": "4.0.0-alpha.10",
    "gsap": "^2.1.3",
    "react-babylonjs": "^1.0.3",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
